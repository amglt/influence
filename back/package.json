  "dependencies": {
    "@prisma/client": "^4.0.0",
    "@types/auth0": "^2.34.19",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.31",
    "auth0": "^2.40.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "express-jwt": "^7.7.1",
